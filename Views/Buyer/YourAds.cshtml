@{
    ViewBag.Title = "YourAds";
}


@model List<Project_Ead_EDEAL.Models.Item>

@{
    Layout = "~/Views/Shared/LayoutOther.cshtml";
}


<div class="col-xs-9">
    <br />
    <br />
    <div>
        <h2>LARGEST SELLING AND BUYING SITE...</h2>
    </div>
    <div>
        <h2 style="color:blue">YOUR ADS ...</h2>
    </div>


    @foreach(var x in Model){
    <div class="navbar navbar-default">
        <div class="col-xs-5">
            <img src="@x.Image" width="300" />
        </div>
        <div class="col-xs-4">
            <h2><a href="/Buyer/ViewAd/@x.Id">@x.Title</a></h2>
            <h3>Price : @x.Price</h3>
            <h3>Cell No : @x.CellNo</h3>
            <h3>City:@x.City</h3>
        </div>
        
    </div>

    }
   
</div>
